@model AboutVM
@{
    ViewData["Title"]="index";
}
<!-- Banner Area Start -->
<div class="banner-area-wrapper">
    <div class="banner-area text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="banner-content-wrapper">
                        <div class="banner-content">
                            <h2>@Model.AboutBanner.FirstTitle</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Banner Area End -->
<!-- About Start -->
<div class="about-area pt-145 pb-155">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-sm-6">
                <div class="about-content">
                    <h2>@Model.AboutBanner.SecondTitle</h2>
                    <p>@Model.AboutBanner.Description</p>
                  
                    <a class="default-btn" href="#">@Model.AboutBanner.ButtonTitle</a>
                </div>
            </div>
            <div class="col-md-6 col-sm-6">
                <div class="about-img">
                    <img src="~/img/about/@Model.AboutBanner.ImageUrl" alt="about">
                </div>
            </div>
        </div>
    </div>
</div>
<!-- About End -->
